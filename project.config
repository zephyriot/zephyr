[project]
	description = Mirror of the zephyr repo on https://gerrit.zephyrproject.org
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Anas Nashif (nashif)
[access "refs/for/refs/*"]
	read = group user/Anas Nashif (nashif)
	push = group user/Anas Nashif (nashif)
	label-Code-Review = -2..+2 group user/Anas Nashif (nashif)
	label-Verified = -1..+1 group user/Anas Nashif (nashif)
	rebase = group user/Anas Nashif (nashif)
	removeReviewer = group user/Anas Nashif (nashif)
	submit = group user/Anas Nashif (nashif)
[access "refs/heads/*"]
	read = group user/Anas Nashif (nashif)
	create = group user/Anas Nashif (nashif)
	push = +force group user/Anas Nashif (nashif)
	pushMerge = group user/Anas Nashif (nashif)
[access "refs/tags/*"]
	push = +force group user/Anas Nashif (nashif)
	pushTag = group user/Anas Nashif (nashif)
	pushSignedTag = group user/Anas Nashif (nashif)
